@{
    ViewBag.Title = "list";
    Layout = "~/Views/Shared/_Loggedin.cshtml";
}

<h2>list</h2>
<table class="table table-bordered">
    <tr><td>Name</td><td>ID</td></tr>
    @for(int i = 0; i < Model.Length; i++)
        {
        <tr><td>@Model[i].Id</td><td>@Model[i].Name</td></tr>
        }
</table>



