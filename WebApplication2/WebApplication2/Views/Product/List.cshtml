@model List<WebApplication2.Models.Product>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List</h2>
<table class="table table-bordered">
    <tr><td>Name</td><td>ID</td></tr>
    @foreach (var p in Model)
    {
        <tr><td>@p.Id</td><td>@p.Name</td></tr>
    }
</table>

